[user]
	name = REMOVED
	email = REMOVED
[core]
	autocrlf = input
	safecrlf = true
	editor = vim
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[alias]
	ls = log --format='%Cgreen%h%Creset %C(cyan)%an%Creset - %s' --graph
[push]
	default = tracking
